<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/stylesheets/profilee.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
</head>
<body>
    <div class="body">
        <div class="navbar">
            <a href="#"><%= user.username%></a>
            <a href="/">All projects </a>
            <a href="/shareProject">Add project</a>
            <a href="/logout">Logout</a>
          </div>

          <div class="content">
            <h1>Your all projects</h1>
            <div class="projects">
          <% user.projects.forEach(function(project) { %>
            <div class="card project" style="width: 18rem;">
              <img src="/images/uploads/<%=project.image%>" class="card-img-top" alt="...">
              <div class="card-body">
                <h5 class="card-title project name"><%= project.projectName %></h5>
                <h5 class="card-title developer name"><%= project.username %></h5>
                <h6 class="card-title developer name"><%= project.dateOfUpload  %></h6>
                <p class="link"><a href="<%=project.projectLink%>" target="_blank">click here to visit project</a></p>
      
                  </div>
            </div>
          <% }); %>
      </div>
  



    </div>

       
        
    
</div>
    
    

    
</body>
</html>